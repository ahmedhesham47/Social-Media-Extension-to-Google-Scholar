@page
@model ScholarStack.Pages.fav_resourcesModel
@{
    ViewData["Title"] = "Favorite Resources";
}

<section>
	<div class="gap gray-bg">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12">
					<div class="row" id="page-contents">
						<div class="col-lg-3">
							<aside class="sidebar static">
								<div class="widget">
									<h4 class="widget-title">Shortcuts</h4>
									<ul class="naves">
										<li>
											<i class="ti-clipboard"></i>
											<a href="index" title="">Community Feed</a>
										</li>
										<li>
											<i class="ti-files"></i>
											<a href="fav-resources" title="">My favorite resources</a>
										</li>
										<li>
											<i class="ti-clipboard"></i>
											<a href="Profile" title="">My Profile</a>
										</li>
										<li>
											<i class="ti-power-off"></i>
											<a href="Login" title="">Log out</a>
										</li>
									</ul>
								</div><!-- Shortcuts -->
							</aside>
						</div><!-- sidebar -->
						<div class="col-lg-6">
							<div class="loadMore">
								@{
									for (int i = 0; i < 5; i++)
									{
										<partial name="Post Elements/_resource-post" />
									}
								}
							</div>
						</div><!-- centerl meta -->
						@{
							<partial name="Widget" />
						}
					</div><!-- sidebar -->
				</div>
			</div>
		</div>
	</div>
	</div>
</section>

