@page
@model ScholarStack.Pages.ResourceFeedModel
@{
	ViewData["Title"] = "Resource Feed";
}

<section>
	<div class="gap gray-bg">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12">
					<div class="row" id="page-contents">
						<div class="col-lg-3">
							<aside class="sidebar static">
								<div class="widget">
									<h4 class="widget-title">Shortcuts</h4>
									<ul class="naves">
										<li>
											<i class="ti-clipboard"></i>
											<a href="index" title="">Community Feed</a>
										</li>
										<li>
											<i class="ti-files"></i>
											<a href="fav-resources" title="">My favorite resources</a>
										</li>
										<li>
											<i class="ti-clipboard"></i>
											<a href="Profile" title="">My Profile</a>
										</li>
										<li>
											<i class="ti-power-off"></i>
											<a href="Login" title="">Log out</a>
										</li>
									</ul>
								</div><!-- Shortcuts -->
							</aside>
						</div><!-- sidebar -->
						<div class="col-lg-6">
							<div class="loadMore">
								@{
									for (int i = 0; i < 5; i++)
									{
										<partial name="Post Elements/_resource-post"/>
									}
								}
							</div>
						</div><!-- centerl meta -->
						<div class="col-lg-3">
							<aside class="sidebar static">
								<div class="widget">
									<h4 class="widget-title">Google Scholar Account</h4>
									<div class="your-page">
										<figure>
											<a href="#" title=""><img src="images/resources/admin2.jpg" alt=""></a>
										</figure>
										<div class="page-meta">
											<a href="https://scholar.google.com.eg/citations?hl=en&user=wejiWMcAAAAJ&view_op=list_works&sortby=pubdate" title="" class="underline">Researcher Profile</a>
										</div>
										<div class="page-likes">
											<ul class="nav nav-tabs likes-btn">
												<li class="nav-item"><a class="active" href="https://scholar.google.com.eg/citations?hl=en&user=wejiWMcAAAAJ&view_op=list_works" data-toggle="tab">Citations</a></li>
												<li class="nav-item"><a class="active" href="https://scholar.google.com.eg/citations?hl=en&user=wejiWMcAAAAJ&view_op=list_works&sortby=pubdate" data-toggle="tab">Latest</a></li>
											</ul>
											<!-- Tab panes -->
											<div class="tab-content">
												<div class="tab-pane active fade show " id="link1">
													<span><i class="fa fa-book"></i>273</span>
												</div>
											</div>
										</div>
									</div>
								</div>
						</div>
						</aside>
					</div><!-- sidebar -->
				</div>
			</div>
		</div>
	</div>
	</div>
</section>
